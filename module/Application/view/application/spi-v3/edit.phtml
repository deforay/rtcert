     function getFacilityInfo(){
        if($('#testingFacility').val() != ''){
            $.blockUI();    
           $.post("<?php echo $this->url('spi-facility', array('action' => 'get-facility-details')); ?>", {id:$('#testingFacility').val()},
           function (data) {
             splitData = data.split('##');
             if(splitData.length == 8){
                $('#testingFacilityName').val(splitData[1]);
                $('#testingFacilityId').val(splitData[0]);
                $('#email').val(splitData[2]);
                $('#contactPerson').val(splitData[3]);
                $('#district').val(splitData[4]);
                $('#province').val(splitData[5]);
                $('#latitude').val(splitData[6]);
                $('#longitude').val(splitData[7]);
                copyFacilityName(splitData[1]);
             }else{
                $('#testingFacilityName').val('');
                $('#testingFacilityId').val('');
                $('#email').val('');
                $('#contactPerson').val('');
                $('#district').val('');
                $('#province').val('');
                $('#latitude').val('');
                $('#longitude').val('');
                copyFacilityName('');
             }
             $.unblockUI();
           });
        }else{
             $('#testingFacilityName').val('');
             $('#testingFacilityId').val('');
             $('#email').val('');
             $('#contactPerson').val('');
             $('#district').val('');
             $('#province').val('');
             $('#latitude').val('');
             $('#longitude').val('');
             copyFacilityName('');
        }
     }
     function showTextBox(obj,textBoxId) {
        if (obj.value=='other') {
            $("#"+textBoxId).show();
            $("#"+obj.id).hide();
        }else{
            $("#"+textBoxId).hide().val('');
            $("#"+obj.id).show();    
        }
      }
