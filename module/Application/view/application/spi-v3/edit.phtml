	function insRow() {
		rowCount++;
		rl = document.getElementById("correctiveActionTable").rows.length;
		var a = document.getElementById("correctiveActionTable").insertRow(rl);
		a.setAttribute("style", "display:none;");
		a.setAttribute("class", "data");
		var b = a.insertCell(0);
		var c = a.insertCell(1);
		var d = a.insertCell(2);
		var e = a.insertCell(3);
		var f = a.insertCell(4);
		var g = a.insertCell(5);
		var h = a.insertCell(6);
		var i = a.insertCell(7);
		
		d.setAttribute("align", "center");
		e.setAttribute("align", "center");
		i.setAttribute("align", "center");
		
		rl = document.getElementById("correctiveActionTable").rows.length - 1;
		b.innerHTML = '<input type="text" class="form-control" id="sectionNo' + rowCount + '" name="sectionNo[]" /><input type="hidden" name="rowId[]" class="form-control" value="'+rowCount+'">';
		c.innerHTML = '<input type="text" class="form-control" id="deficiency' + rowCount + '" name="deficiency[]" />';
		d.innerHTML = '<input type="radio" class="isRequired" name="correction'+rowCount+'" value="Immediate" title="Please select the corrective action">';
		e.innerHTML = '<input type="radio" class="isRequired" name="correction'+rowCount+'" value="Followup" title="Please select the corrective action">';
		f.innerHTML = '<textarea class="form-control" name="auditorComment[]"></textarea>';
		g.innerHTML = '<textarea class="form-control" name="action[]"></textarea>';
		h.innerHTML = '<input type="text" name="timeline[]" class="form-control">';
		i.innerHTML = '<a class="btn btn-xs btn-success" href="javascript:void(0);" onclick="insRow();"><i class="fa fa-plus"></i></a> <a class="btn btn-xs btn-warning" href="javascript:void(0);" onclick="removeRow(this.parentNode.parentNode);"><i class="fa fa-minus"></i></a>';
		$(a).fadeIn(800);
		
	}
	
	function removeRow(el) {
		$(el).fadeOut("slow", function() {
		    el.parentNode.removeChild(el);
		    rl = document.getElementById("correctiveActionTable").rows.length;
		    if (rl == 2) {
			insRow();
		    }
		});
     }
     
